{
  "name": "shoppyreact",
  "version": "1.0.0",
  "description": "Store manager react version",
  "main": "index.js",
  "directories": {
    "test": "jest"
  },
  "scripts": {
    "build": "webpack --config webpack/webpack.prod.js",
    "start": "node ./server.js",
    "dev": "webpack-dev-server --config webpack/webpack.dev.js --open --hot",
    "test": "NODE_ENV=test jest --coverage",
    "lint": "./node_modules/eslint/bin/eslint.js . --ext .js,.jsx",
    "heroku-postbuild": "npm run build"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./enzyme.setup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coveragePathIgnorePatterns": [
      "helpers"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tosinibrahim96/shoppyReact.git"
  },
  "keywords": [
    "andela",
    "react",
    "redux",
    "bootcamp"
  ],
  "author": "Ibrahim Alausa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tosinibrahim96/shoppyReact/issues"
  },
  "homepage": "https://github.com/tosinibrahim96/shoppyReact#readme",
  "dependencies": {
    "@babel/polyfill": "7.2.5",
    "@babel/runtime": "7.3.1",
    "autoprefixer": "9.4.7",
    "axios": "0.18.0",
    "enzyme": "3.8.0",
    "express": "4.16.4",
    "imagemin-pngquant": "5.0.1",
    "jwt-decode": "2.2.0",
    "normalize.css": "8.0.1",
    "postcss-loader": "3.0.0",
    "prop-types": "15.7.2",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-redux": "6.0.0",
    "react-responsive-carousel": "3.1.47",
    "react-responsive-modal": "3.6.0",
    "react-router-dom": "4.3.1",
    "react-toastify": "4.5.2",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "redux-mock-store": "1.5.3",
    "redux-thunk": "2.3.0",
    "semantic-ui-css": "2.4.1",
    "semantic-ui-react": "0.85.0",
    "sinon": "7.2.3",
    "validator": "10.11.0",
    "webpack": "4.29.0",
    "webpack-cli": "3.2.1",
    "webpack-dev-server": "3.1.14",
    "webpack-merge": "4.2.1"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.3.0",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.0.0",
    "babel-loader": "8.0.5",
    "check-prop-types": "1.1.2",
    "clean-webpack-plugin": "1.0.1",
    "css-loader": "2.1.0",
    "dotenv-webpack": "1.7.0",
    "enzyme-adapter-react-16": "1.8.0",
    "enzyme-to-json": "3.3.5",
    "eslint": "5.13.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "3.0.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.3.1",
    "image-webpack-loader": "4.6.0",
    "jest": "24.0.0",
    "jest-html-reporter": "2.4.4",
    "jest-localstorage-mock": "2.4.0",
    "mini-css-extract-plugin": "0.5.0",
    "moxios": "0.4.0",
    "node-sass": "4.11.0",
    "nyc": "13.2.0",
    "react-test-renderer": "16.7.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "url-loader": "1.1.2"
  }
}
